  <ItemGroup>
    <Compile Remove="NGeoKDBush.Test\**" />
    <Compile Remove="NGeoKDBush.Test\Data\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="NGeoKDBush.Test\**" />
    <EmbeddedResource Remove="NGeoKDBush.Test\Data\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="NGeoKDBush.Test\**" />
    <None Remove="NGeoKDBush.Test\Data\**" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="NGeoKDBush.Test" />
  </ItemGroup>
